/* components/custom-nav.wxss */
.custom-nav{
  position: fixed;
  top:0;
  left:0;
  background: #fff;
  width: 100%;
  padding-bottom: 10rpx;
}
.custom-nav-title{
  font-size: 38rpx;
  color: #333;
  font-weight: bold;
}
.posCenter{
  text-align: center;
}

.posLeft {
  text-align: left;
}
.route-back{
  position: absolute;
  left:0;
  top: 0; 
  box-sizing: border-box;
  transform: rotateZ(90deg);
}